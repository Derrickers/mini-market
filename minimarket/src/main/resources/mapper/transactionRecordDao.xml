<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 设置为UserDao接口方法提供sql语句配置 -->
<mapper namespace="com.minimarket.dao.transactionRecordDao">
<!--通过钱包地址获得密码-->
<select id="getPasswordByAddress" resultType="String" parameterType="com.minimarket.model.transactionRecord">
        SELECT password FROM user_info WHERE Address = #{Address}
    </select>
</mapper>